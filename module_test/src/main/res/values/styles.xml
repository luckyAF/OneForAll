<resources>

    <!-- Base application theme. -->
    <style name="TestAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:textAllCaps">false</item>
        <item name="colorPrimary">@color/test_colorPrimary</item>
        <item name="colorPrimaryDark">@color/test_colorPrimaryDark</item>
        <item name="colorAccent">@color/test_colorAccent</item>
    </style>

    <style name="TestAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="TestAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="TestAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
